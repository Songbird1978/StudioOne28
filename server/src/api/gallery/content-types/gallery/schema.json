{
  "kind": "collectionType",
  "collectionName": "galleries",
  "info": {
    "singularName": "gallery",
    "pluralName": "galleries",
    "displayName": "gallery"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
