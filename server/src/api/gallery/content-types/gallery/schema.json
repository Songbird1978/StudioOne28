      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "galleryName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "galleryName"
    },
    "GalleryCategory": {
      "type": "enumeration",
      "enum": [
        "Live Drums",
        "Studio Drums",
        "Discography",
        "Studio One28",
        "Equipment",
        "Facilities"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "gallery"
    }
  }
}
